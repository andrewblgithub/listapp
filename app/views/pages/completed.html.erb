<div class="row">
    <div class="col-sm-4">
        <%= render 'layouts/groups' %>
    </div>
    <div class="col-sm-8">
        <% Post.all.each do |post| %>
            <% if post.expires_at <= Time.now && post.list.user_id == current_user.id %>
            <%= render 'posts/posts', :post => post %>
            <% end %>
        <% end %>
        <hr>
    </div>
</div>